                            <span class="order-details-text">{{ order_name }}</span>
                        </li>
                        {% for attribute in attributes %}
                          {% if attribute.first contains 'PO' %}
                            {% if attribute.last != blank %}
                              <li class="orders-details-po">
                                <span class="order-details-title editable" data-key="po_number">
                                  {{ attribute | first | remove: "Manual PO" }} 
                                </span>
                                <span class="order-details-text">
