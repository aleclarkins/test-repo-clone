                            </tr> -->
                        {% endfor %}
                        <tr class="pricing-table-total-row">
                            <td style="background: #353842;" class="pricing-table-title"><span style="text-transform: capitalize; color: #ffffff;" class="editable" data-key="total">Grand Total</span></td>
                            <td style="background: #353842; color: #ffffff;" class="pricing-table-text">{{ total_price | money }}</td>
                        </tr>
                        {% assign total_refunds = 0.0 %}
